<!--pages/out/out.wxml-->
<!--pages/in/in.wxml-->
<view class="container"> 
  <view class="inapply">出校申请</view>
  <text class="line">\n</text>
  <view class="item">
    <view class="info">
      <view class="inschool">入校时间：{{inschool}}</view>
      <view class="outschool">出校时间：{{outschool}}</view>
      <view class="trueinschool">实际入校时间：{{trueinschool}}</view>
    </view>
    <view class="bt_box">
      <button bindtap="outschool" class="bt" size="mini">点击出校</button>
    </view>
  </view>
</view>

