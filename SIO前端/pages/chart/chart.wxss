.echart_wrap{
  width: 100%;
  height: 680rpx;
}
ec-canvas {
  width: 100%;
  height: 100%;
}

.divLine {
  background: #F4F4F4;
  width: 100%;
  height: 20rpx;
}
.container { 
  height: 100%; 
  display: flex; 
  flex-direction: column; 
  padding: 0; 
  box-sizing: border-box; 
 } 
 .title{
   width: 100%;
   height: 80rpx;
   font-size: xx-large;
   margin-top: 20rpx;
   margin-bottom: 10rpx;
   border: aqua;
   text-align: center;
  background-color: #f2f2f2;
 }
 .line{
   width: 100%;
   height: 3rpx;
   background-color:#dddbdb ;
 }
 .search{
   margin: 20rpx;
   background-color: #f2f2f2;
   width: 100%;
 }
 .searchbox{
   display: flex;
   flex-direction: row;
 }
 .searchLab{
   width: 15%;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color: #545454; 
  font-size: 14px
 }
 .inputText { 
  flex: block; 
  text-align: left; 
  margin-right: 22px; 
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color: #cccccc; 
  font-size: 14px;
  width:50%
 } 
 .dateinputText { 
  flex: block; 
  text-align: left; 
  margin-right: 10px; 
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color: #cccccc; 
  font-size: 14px;
  width:10%
 } 
.searchbtbox{
  display: flex;
  flex: row;
  position: relative;
  left: 350rpx;
}
 .bt_box{
   margin-top: 10rpx;
   width: 25%;
 }
 .bt{
  background-color: rgb(69, 188, 218);
  color: white;
  border-radius: 98rpx; 
 }
.menu{
  
  width: 100%;
  height: 8%;
  display: flex;
  flex: row;
  background-color: #ffffff;
}
.menuitem{
  width: 40%;
  margin-right: 5%;
  margin-left: 5%;
  height: 7%;
  margin-top: 0.5%;
  margin-bottom: 0.5%;
  display:flex;
  justify-content:center;

}
.menutext{
  color: #aaaaaa;
  height: 7%;
  display:flex;
  justify-content:center;
}
.menutext2{
  height: 7%;
  display:flex;
  justify-content:center;
}
.icon{
  width: 50rpx;
  height: 50rpx;
}